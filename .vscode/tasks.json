    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build vole",
            "command": "python3",
            "args" :[
             "build.py",
             "-DVOLE_PSI_ENABLE_BOOST=ON",
             //"-DVOLE_PSI_ENABLE_OPENSSL=ON",
             //"-DVOLE_PSI_ENABLE_GMW=OFF",
             //"-DVOLE_PSI_ENABLE_CPSI=OFF",
             //"-DVOLE_PSI_ENABLE_OPPRF=OFF",
             //"-CMAKE_BUILD_TYPE=Debug",
             //"--clean",
             "--debug",     
            ],
            "type": "shell",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "new",
                "focus": true
            }
        }
    ]
