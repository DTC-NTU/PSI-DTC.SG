{
    "configurations": [
        {
            "name": "(gdb) Launch vole test",
            "preLaunchTask": "Build vole",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/linux/frontend/frontend",
            "args": ["-u","54"],  
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
